<tr>
  <td><%= food.id %></td>
  <td><%= food.name %></td>
  <td><%= food.created_at.localtime.rfc822 %></td>
  <td>
    <%= link_to "View", manage_food_path(food), :class => "btn btn-default", :method => :get  %>
    <%= link_to "Edit", edit_manage_food_path(food), :class => "btn btn-default" %>
    <%= link_to "Delete", manage_food_path(food), :class => "btn btn-danger", :method => :delete , data: { confirm: "Are you sure?" } %>
  </td>
</tr>
